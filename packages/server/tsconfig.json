{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "./dist",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "rootDir": "./src",
    "moduleResolution": "Node"
  },
  "references": [],
  "include": ["src/**/*"],
  "exclude": ["./dist", "./node_modules"]
}
